<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    <servlet>
        <servlet-name>details</servlet-name>
        <servlet-class>com.objectfrontier.servlet.UserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>credential</servlet-name>
        <servlet-class>com.objectfrontier.servlet.CredentialServlet</servlet-class>
    </servlet>
    
     <servlet>
        <servlet-name>product</servlet-name>
        <servlet-class>com.objectfrontier.servlet.productServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>details</servlet-name>
        <url-pattern>/userDetail</url-pattern>
    </servlet-mapping> 
    
    <servlet-mapping>
        <servlet-name>credential</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>product</servlet-name>
        <url-pattern>/productDetails</url-pattern>
    </servlet-mapping>

</web-app>
